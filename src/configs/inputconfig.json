{
  "auth": {
    "login": {
      "maxLength": 256
    },
    "password": {
      "maxLength": 64,
      "minLength": 4
    }
  },
  "contingent": {
    "code": {
      "maxLength": 10,
      "pattern": ["^[0-9\\p{L}]+$", "u"]
    },
    "desc": {
      "maxLength": 256
    }
  },
  "employee": {
    "birthdate": {
      "min": "1900-01-01"
    },
    "name": {
      "maxLength": 256,
      "pattern": [
        "^\\p{Script=Cyrillic}(?:[\\p{Script=Cyrillic}ivx]*(?:(?<=[\\p{Script=Cyrillic}ivx])[-_,.' ](?=[\\p{Script=Cyrillic}ivx]))?)*$",
        "ui"
      ]
    },
    "password": {
      "maxLength": 32,
      "minLength": 6,
      "pattern": "^[\\w.,:;?!*+%-@$#]+$"
    }
  },
  "examination": {
    "date": {
      "min": "1900-01-01"
    },
    "name": {
      "maxLength": 256,
      "pattern": [
        "^\\p{L}(?:\\p{L}*(?:(?<=\\p{L})[-_,.' ](?=\\p{L}))?)*$",
        "ui"
      ]
    },
    "tests": {
      "maxSize": 10
    },
    "text": {
      "maxLength": 256
    }
  },
  "lpu": {
    "abbr": {
      "maxLength": 256
    },
    "opf": {
      "maxLength": 32,
      "pattern": ["^\\p{Script=Cyrillic}+(?:\\s\\p{Script=Cyrillic}+)*$", "u"]
    }
  }
}
