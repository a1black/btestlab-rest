{
  "contingent": {
    "code": {
      "maxLength": 10,
      "pattern": ["^[0-9\\p{L}]+$", "u"]
    },
    "desc": {
      "maxLength": 256
    }
  },
  "employee": {
    "name": {
      "maxLength": 256,
      "pattern": [
        "^\\p{Script=Cyrillic}(?:[\\p{Script=Cyrillic}ivx]*(?:(?<=[\\p{Script=Cyrillic}ivx])[-_,.' ](?=[\\p{Script=Cyrillic}ivx]))?)*$",
        "ui"
      ]
    },
    "password": {
      "maxLength": 32,
      "minLength": 6,
      "pattern": "^[\\w!#$%&?@^=+-]+$"
    }
  }
}
