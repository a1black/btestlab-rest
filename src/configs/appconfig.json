{
  "server": {
    "logLevel": "error",
    "port": 80
  },
  "accessToken": {
    "algorithm": "HS256",
    "expiresIn": 86400,
    "issuer": "https://voib2lab.ru"
  },
  "db": {
    "dbname": "exam_journal",
    "uri": "mongodb://mongodb:27017"
  },
  "general": {
    "passwdHashSize": 32
  },
  "genops": {
    "employeeId": {
      "length": 10,
      "prefix": 3400
    },
    "lpuId": {
      "length": 6,
      "prefix": 3400
    }
  },
  "routes": {
    "auth": "/auth",
    "contingent": "/contingent",
    "employee": "/employee",
    "examination": "/exam",
    "lpu": "/lpu"
  }
}
