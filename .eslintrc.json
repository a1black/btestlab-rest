{
  "env": {
    "es2021": true,
    "mongo": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:promise/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["node", "promise"],
  "root": true,
  "rules": {
    "no-ex-assign": "off",
    "node/no-unsupported-features/es-syntax": "off"
  }
}
